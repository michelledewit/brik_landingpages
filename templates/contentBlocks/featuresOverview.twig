<!-- methodisch -->
{% set features = contentBlock.profileM.featureOverview.all() %}
<div id="features" x-show="methodicScore > competitiveScore && methodicScore > spontaniousScore && methodicScore > humanisticScore && methodicScore >= 20" class="bg-white">
	<div class="max-w-7xl mx-auto py-12 px-4 sm:px-6 lg:px-12">
		<div class="mx-auto max-w-5xl w-3/4 mb-4 text-center text-black">
			<h2 class="text-3xl tracking-tight mb-2">{{contentBlock.profileM.overviewTitle}}</h2>
			<p class="text-lg text-center leading-6">{{contentBlock.profileM.overviewSubtitle}}</p>
		</div>
		<div class="mt-8 space-y-4 sm:space-y-0 sm:grid sm:grid-cols-2 sm:gap-x-6 sm:gap-y-12 lg:grid-cols-3 lg:gap-x-8">

			{% for feature in features %}
				<div class="relative">
					<div>
						<svg class="absolute h-6 w-6 text-red-500" xmlns="http://www.w3.org/2000/svg" fill="none" viewbox="0 0 24 24" stroke="currentColor" aria-hidden="true">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
						</svg>
						<p class="ml-9 text-base leading-5">{{feature.featureTitle}}</p>
					</div>
					<div class="mt-2 ml-9 text-xs">{{feature.featureText}}</div>
				</div>
			{% endfor %}

		</div>
	</div>
</div>

<!-- spontaan-->
{% set features = contentBlock.profileS.featureOverview.all() %}
<div id="features" x-show="spontaniousScore > competitiveScore && spontaniousScore > humanisticScore && spontaniousScore > methodicScore && spontaniousScore >= 20" class="">
	<div class="mx-auto text-center relative bg-white max-w-7xl mx-auto px-4 my-10 sm:px-6 lg:px-12">
		<div class="mx-auto max-w-5xl w-3/4 mb-4 text-center text-black">
			<h2 class="text-3xl tracking-tight mb-2">{{contentBlock.profileS.overviewTitle}}</h2>
		</div>
		<div class="mt-6">
			<div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-3">
				{% for feature in features %}
					<div class="pt-6 ">
						<div data-ls='{"mouseover":[0,0,1,0,1], "click":[0,0,0,0,0]}' class="flow-root rounded-lg bg-gray-100 px-3 pb-4 h-32">
							<div class="-mt-6 ">
								<div>
									<span class="mt-2 inline-flex items-center justify-center rounded-md bg-gray-300 p-2 shadow-lg">
										<svg class="h-4 w-4 text-black" xmlns="http://www.w3.org/2000/svg" fill="none" viewbox="0 0 24 24" stroke="currentColor" aria-hidden="true">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"/>
										</svg>
									</span>
								</div>
								<h3 class="mt-3 text-base leading-5 px-2">{{feature.featureTitle}}</h3>
								<p class="mt-1 text-xs text-gray-800">{{feature.featureText}}</p>
							</div>
						</div>
					</div>
				{% endfor %}
			</div>
		</div>
	</div>
</div>

