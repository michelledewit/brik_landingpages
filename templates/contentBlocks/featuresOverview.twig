<!-- humanistisch-->
{% set features = contentBlock.profileH.featureOverview.all()|slice(0,3) %}
{% set features2 = contentBlock.profileH.featureOverview.all()|slice(3,3) %}
<div id="features" x-show="humanisticScore >= competitiveScore && humanisticScore >= spontaniousScore && humanisticScore >= methodicScore || spontaniousScore < 5 && competitiveScore < 5 && methodicScore < 5 " class="">
	<div class="mx-auto text-center relative bg-white max-w-7xl mx-auto px-4 my-10 sm:px-6 lg:px-12">
		<div class="mx-auto max-w-5xl w-3/4 mb-4 text-center text-black">
			<h2 class="text-3xl tracking-tight mb-2">{{contentBlock.profileH.overviewTitle}}</h2>
			<p class="text-lg text-center leading-6">{{contentBlock.profileH.overviewSubtitle}}</p>
		</div>
		<div class="mt-6">
			<div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-3">
				{% for feature in features %}
					<div class="pt-6 ">
						<div class="flow-root rounded-lg bg-gray-100 px-3 pb-4 h-32">
							<div class="-mt-6 ">
								<div>
									<span class="mt-2 inline-flex items-center justify-center rounded-md bg-gray-300 p-2 shadow-lg">
										<svg data-ls='{"mouseover":[1,1,1,1,1], "click":[2,2,2,2,2]}' class="h-4 w-4 text-black" xmlns="http://www.w3.org/2000/svg" fill="none" viewbox="0 0 24 24" stroke="currentColor" aria-hidden="true">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"/>
										</svg>
									</span>
								</div>
								<h3 class="mt-3 text-base leading-5 px-2">{{feature.featureTitle}}</h3>
								<p class="mt-1 text-xs text-gray-800">{{feature.featureText}}</p>
							</div>
						</div>
					</div>
				{% endfor %}
			</div>
			<div x-data="{ open: false }">
				<div class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-3 mt-2">
					{% for feature in features2 %}
						<div class="pt-6" :class="open ? 'pt-6' : 'hidden'">
							<div class="flow-root rounded-lg bg-gray-100 px-3 pb-4 h-32">
								<div class="-mt-6 ">
									<div>
										<span class="mt-2 inline-flex items-center justify-center rounded-md bg-gray-300 p-2 shadow-lg">
											<svg class="h-4 w-4 text-black" xmlns="http://www.w3.org/2000/svg" fill="none" viewbox="0 0 24 24" stroke="currentColor" aria-hidden="true">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"/>
											</svg>
										</span>
									</div>
									<h3 class="mt-3 text-base leading-5 px-2">{{feature.featureTitle}}</h3>
									<p class="mt-1 text-xs text-gray-800">{{feature.featureText}}</p>
								</div>
							</div>
						</div>
					{% endfor %}
				</div>
				<div @click="open = ! open" :class="open ? 'hidden' : 'mx-auto flex justify-center mb-8'">
					<a data-ls='{"mouseover":[1,1,1,1,1], "click":[2,2,2,2,2]}' class="mt-4 w-full inline-flex items-center justify-center px-6 py-1 border border-transparent font-medium text-white bg-gray-500 hover:opacity-80 sm:w-auto text-xs">
					Meer tonen
					</a>
				</div>
			</div>
		</div>
	</div>
</div>
