<!-- methodisch / humanistisch -->
<div x-show="methodicScore > competitiveScore && methodicScore > spontaniousScore && methodicScore > humanisticScore && methodicScore >= 20 || humanisticScore > competitiveScore && humanisticScore > spontaniousScore && humanisticScore > methodicScore && humanisticScore >= 20" class="bg-gray-200 my-8">
	<div class="max-w-7xl mx-auto py-8 px-4 sm:px-6 lg:px-12">
		<div class="lg:grid lg:grid-cols-2 lg:gap-8 lg:items-center">
			<div>
				<h2 class="text-3xl text-black leading-7">{{contentBlock.blockTitle}}</h2>
				<p class="mt-3 max-w-3xl text-sm text-black">{{contentBlock.blockText}}</p>
				<div class="mt-4">
					{% if contentBlock.blockLink and contentBlock.blockLink.url %}
						<a data-ls='{"mouseover":[1,1,-1,-1,1], "click":[2,2,-2,-2,2]}' href="#whitepaper" class="w-full inline-flex items-center justify-center px-5 py-1 border border-transparent font-medium text-black bg-white hover:opacity-80 hover:text-black sm:w-auto text-sm">
							{{ contentBlock.blockLink.text }}
						</a>
					{% endif %}
				</div>
			</div>
			<div class="mt-8 grid grid-cols-2 gap-2 md:grid-cols-3 lg:mt-0 lg:grid-cols-2">
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/transistor-logo-gray-400.svg" alt="Workcation">
				</div>
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/mirage-logo-gray-400.svg" alt="Mirage">
				</div>
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/tuple-logo-gray-400.svg" alt="Tuple">
				</div>
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/laravel-logo-gray-400.svg" alt="Laravel">
				</div>
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/statickit-logo-gray-400.svg" alt="StaticKit">
				</div>
				<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="col-span-1 flex justify-center py-4 px-8 bg-gray-50">
					<img class="max-h-12" src="https://tailwindui.com/img/logos/statamic-logo-gray-400.svg" alt="Statamic">
				</div>
			</div>
		</div>
	</div>
</div>

<!-- spontaan / competitief -->
<div x-show="spontaniousScore >= competitiveScore && spontaniousScore >=  humanisticScore && spontaniousScore >= methodicScore || humanisticScore < 20 && competitiveScore < 20 && methodicScore < 20 || competitiveScore >= humanisticScore && competitiveScore >= spontaniousScore && competitiveScore >= methodicScore || spontaniousScore < 20 && humanisticScore < 20 && methodicScore < 20" class="my-8">
	<div class="max-w-7xl mx-auto mt-12 px-4 sm:px-6 lg:px-12 bg-gray-200 py-6">
		<div class="lg:grid lg:grid-cols-2 lg:gap-2">
			<div class="grid content-center">
				<h2 class="max-w-md mx-auto text-3xl text-black text-center leading-7 lg:max-w-xl lg:text-left">
					{{contentBlock.blockTitle}}</h2>
			</div>
			<div class="flow-root self-center mt-8 lg:mt-0">
				<div class="-mt-4 -ml-8 grid grid-cols-2 md:grid-cols-3 lg:mt-0 lg:grid-cols-2 lg:ml-2">
					<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="mt-2 bg-white py-4 px-1 flex flex-grow flex-shrink-0 justify-center lg:flex-grow-0 ml-2">
						<img class="h-7" src="https://tailwindui.com/img/logos/laravel-logo-gray-400.svg" alt="Workcation">
					</div>
					<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="mt-2 bg-white py-4 px-1 flex flex-grow flex-shrink-0 justify-center lg:flex-grow-0 ml-2">
						<img class="h-7" src="https://tailwindui.com/img/logos/tuple-logo-gray-400.svg" alt="Tuple">
					</div>
					<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="mt-2 bg-white py-4 px-1 flex flex-grow flex-shrink-0 justify-center lg:flex-grow-0 ml-2">
						<img class="h-7" src="https://tailwindui.com/img/logos/statamic-logo-gray-400.svg" alt="Level">
					</div>
					<div data-ls='{"mouseover":[1,1,-1,1,-1], "click":[0,0,0,0,0]}' class="mt-2 bg-white py-4 px-1 flex flex-grow flex-shrink-0 justify-center lg:flex-grow-0 ml-2">
						<img class="h-7" src="https://tailwindui.com/img/logos/statamic-logo-gray-400.svg" alt="Level">
					</div>
				</div>
			</div>
		</div>
	</div>
</div>


