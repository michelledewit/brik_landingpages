{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-seomatic": "3.4.29",
    "nystudio107/craft-imageoptimize": "^1.4.35",
    "nystudio107/craft-minify": "^1.2.9",
    "craftcms/redactor": "^2.1.2",
    "rias/craft-position-fieldtype": "^1.0.13",
    "aelvan/imager": "^2.0.2",
    "mikestecker/craft-videoembedder": "^1.0.9",
    "fruitstudios/linkit": "^1.0.8",
    "verbb/navigation": "^1.0.18",
    "craftcms/contact-form": "^2.2.3",
    "putyourlightson/craft-snaptcha": "^2.1.0",
    "craftcms/mailgun": "^1.3.0",
    "nystudio107/craft-typogrify": "^1.1",
    "verbb/super-table": "2.6.8",
    "nystudio107/craft-cookies": "1.1.15"
  },
  "autoload": {
    "psr-4": {
      "modules\\profiledatamodule\\": "modules/profiledatamodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
